<div class="container px-5 px-sm-0">
<div class="row">
  <div class="col-md-3">
    </br>
    <h2>新規登録リンク</h2>
    </br>

    
  </div>

    <div class="col-md-8 offset-md-1" >
      </br>
      <h2>Genres</h2>
        <table class="table table-hover">
          <thead>
            <tr>
              <th>Genre_name</th>
              <th>登録日時</th>
              <th>更新日時</th>
            </tr>
          </thead>
          <tbody>
          <% @genres.each do |genre| %>
          <tr>
           <td><%= genre.name %></td>
           <td><%= genre.created_at.strftime("%Y-%m-%d %H:%M") %></td>
           <td><%= genre.updated_at.strftime("%Y-%m-%d %H:%M") %></td>
          </tr>
          <% end %>
          </tbody>
        </table>
    </div>
  </div>
 </div>