<div class="row">
<h2 class="genre-edit-head">ジャンル編集</h2>
<%= form_with model: @genre, url: admin_genre_path(@genre.id), method: :PATCH do |genre| %>
	<%= genre.text_field :name %>
	<label>
	<%= genre.radio_button :is_enabled, :true %>有効
	</label>
	<label>
	<%= genre.radio_button :is_enabled, :false %>無効
	</label>
	<%= genre.submit '変更を保存する',class:"btn btn-success genre-submit" %>
<% end %>
</div>
